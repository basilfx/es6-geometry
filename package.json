{
  "name": "es6-geometry",
  "version": "2.0.0",
  "description": "Collection of immutable ES6 classes for simple 2D geometric objects and calculations.",
  "main": "lib/index.js",
  "directories": {
    "test": "test",
    "lib": "lib"
  },
  "scripts": {
    "build": "npm run build-src",
    "build-src": "babel -d lib/ src/",
    "build-docs": "esdoc -c esdoc.json",
    "bundle": "npm run bundle-prod && npm run bundle-dev",
    "bundle-prod": "NODE_ENV=production webpack --config webpack.config.production.js",
    "bundle-dev": "webpack --config webpack.config.js",
    "lint": "eslint src/ test/",
    "test": "mocha --require babel-register",
    "prepublish": "npm run build && npm run bundle"
  },
  "keywords": [
    "geometry",
    "es6"
  ],
  "author": "Bas Stottelaar",
  "license": "MIT",
  "devDependencies": {
    "babel-cli": "^6.25.0",
    "babel-core": "^6.25.0",
    "babel-eslint": "^8.0.1",
    "babel-loader": "^7.1.2",
    "babel-preset-es2015": "^6.9.0",
    "babel-preset-stage-0": "^6.5.0",
    "babili-webpack-plugin": "^0.1.2",
    "esdoc-ecmascript-proposal-plugin": "^1.0.0",
    "esdoc-flow-type-plugin": "^1.0.0",
    "esdoc-jsx-plugin": "^1.0.0",
    "esdoc-standard-plugin": "^1.0.0",
    "esdoc": "^1.0.0",
    "eslint-config-semistandard": "^11.0.0",
    "eslint-config-standard": "^10.2.0",
    "eslint-import-resolver-webpack": "*",
    "eslint-plugin-babel": "*",
    "eslint-plugin-flowtype-errors": "^3.2.0",
    "eslint-plugin-flowtype": "^2.30.4",
    "eslint-plugin-import": "*",
    "eslint-plugin-node": "^5.1.0",
    "eslint-plugin-promise": "^3.5.0",
    "eslint-plugin-standard": "^3.0.1",
    "eslint": "^4.8.0",
    "flow-bin": "^0.56.0",
    "mocha": "^4.0.0",
    "webpack": "^3.6.0"
  },
  "repository": {
    "type": "git",
    "url": "git://github.com/basilfx/es6-geometry.git"
  }
}
